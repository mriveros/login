<h1>Listing telefono_personas</h1>

<table>
  <tr>
    <th>Nombre</th>
    <th>Telefono</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @telefono_personas.each do |telefono_persona| %>
  <tr>
    <td><%= telefono_persona.nombre %></td>
    <td><%= telefono_persona.telefono %></td>
    <td><%= link_to 'Show', telefono_persona %></td>
    <td><%= link_to 'Edit', edit_telefono_persona_path(telefono_persona) %></td>
    <td><%= link_to 'Destroy', telefono_persona, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

